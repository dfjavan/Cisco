username admin secret Mypass                              old methud
username admin algorithm-type sha256 secret MyPassword    new methud


(config)# ip domain-name resaa.net
(config)# crypto key generate rsa

(config)#line vty 0
(config-line)# privilege level 15
enable secret Mypass

(config-line)#login local
transport input ssh
(config)#ip ssh version 2

(config)#line console 0
(config-line)# privilege level 15
(config-line)#login local

(config)#interface vlan 1
ip address 192.168.20.200 255.255.255.0

(config)# ip default-gateway 192.168.1.1
